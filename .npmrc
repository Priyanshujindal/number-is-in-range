# NPM Configuration for number-is-in-range package

# Ensure package is published as public
access=public

# Set registry to npm
registry=https://registry.npmjs.org/

# Enable strict SSL
strict-ssl=true

# Set package visibility
package-lock=true

# Enable audit
audit=true

# Set fund message
fund=false

# Set save exact versions
save-exact=true

# Set engine strict
engine-strict=true

# Set progress bar
progress=true

# Set log level
loglevel=warn

# Set cache
cache=.npm-cache

# Set tmp
tmp=.npm-tmp

# Set prefix
prefix=.

# Set init settings
init.author.name=Priyanshu Jindal
init.author.email=priyanshu.jindal@example.com
init.author.url=https://github.com/Priyanshujindal
init.license=MIT
init.version=1.0.0 